        "i18next-browser-languagedetector": "^8.0.4",
        "jest": "^29.7.0",
        "jszip": "^3.10.1",
        "mp4-muxer": "^5.2.0",
        "playcanvas": "^2.5.1",
        "postcss": "^8.5.3",
        "rollup": "^4.34.8",
        "rollup-plugin-scss": "^4.0.1",
        "rollup-plugin-string": "^3.0.0",
        "rollup-plugin-visualizer": "^5.14.0",
        "sass": "^1.85.1",
        "serve": "^14.2.4",
        "tslib": "^2.8.1",
        "typescript": "^5.7.3"
    }
